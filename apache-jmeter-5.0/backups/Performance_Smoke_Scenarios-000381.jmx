long totalRequest;
long currentRequestRT = prev.getTime();
log.info(&quot;response time: &quot;+ currentRequestRT);
if(currentRequestRT!= null)
{
    rtList.add(currentRequestRT);    
}

int len = rtList.size();
log.info(&quot;lenght of the list: &quot;+ len);
totalRequest=len;
log.info(&quot;Total Requests: &quot;+ totalRequest);
for(int j=0; j&lt;len; j++){
    totalRT = totalRT+rtList.get(j);
}

log.info(&quot;Total response time: &quot;+ totalRT );

long avgRT = totalRT / totalRequest;
log.info(&quot;HHIIiiiiiiiiiiiii verifyingggggg&quot;);
vars.put(&quot;average&quot;,String.valueOf(avgRT));
